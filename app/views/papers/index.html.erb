<h1>Listing papers</h1>

<table>
  <tr>
    <th>Title</th>
     <th>Venue</th>
    <th>Year</th>
    <th>Links</th>
  </tr>
  <% @papers.each do |paper| %>
      <tr>
        <td><%= paper.title %></td>
        <td><%= paper.venue %></td>
        <td><%= paper.year %></td>
        <td><%= link_to 'Show', paper_path(paper) %></td>
        <td><%= link_to 'Edit', edit_paper_path(paper) %></td>
        <td><%= link_to 'Destroy', paper_path(paper),
                        method: :delete %></td>
      </tr>
  <% end %>

  <%= link_to 'Add paper', new_paper_path %>
</table>